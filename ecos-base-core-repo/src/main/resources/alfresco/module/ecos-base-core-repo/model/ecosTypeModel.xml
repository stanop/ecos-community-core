<?xml version="1.0" encoding="UTF-8"?>
<model name="etype:model" xmlns="http://www.alfresco.org/model/dictionary/1.0">

    <description>ECOS type model</description>
    <author>Citeck</author>
    <version>1.0</version>

    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
    </imports>

    <namespaces>
        <namespace uri="http://www.citeck.ru/model/ecos-type/1.0" prefix="ecosType"/>
    </namespaces>

    <aspects>
        <aspect name="ecosType:hasType">
            <title>Has ECOS type</title>
            <properties>
                <property name="ecosType:type">
                    <type>d:text</type>
                </property>
            </properties>
        </aspect>
    </aspects>

</model>
